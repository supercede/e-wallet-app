{
  "name": "e-wallet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s",
    "clean": "rm -rf .nyc_output build coverage && mkdir build",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "dev": "cross-env DEBUG=dev nodemon --exec babel-node src/index.js",
    "lint": "eslint \"**/*.js\" --fix",
    "postinstall": "npm run build && npm run db:migrate",
    "posttest": "cross-env NODE_ENV=test npm run db:migrate:undo",
    "pretest": "cross-env NODE_ENV=test npm run db:migrate:undo && cross-env NODE_ENV=test npm run db:migrate",
    "start": "node ./build/server.js",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=1 --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "sequelize": "^6.2.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "jest": "^26.1.0",
    "sequelize-cli": "^6.1.0",
    "supertest": "^4.0.2"
  }
}
